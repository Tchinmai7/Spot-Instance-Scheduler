<%= form_for(aws_key) do |f| %>
  <% if aws_key.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(aws_key.errors.count, "error") %> prohibited this aws_key from being saved:</h2>

      <ul>
      <% aws_key.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>

  <div class="field">
    <%= f.label :accessKey %>
    <%= f.text_field :accessKey %>
  </div>

  <div class="field">
    <%= f.label :secretKey %>
    <%= f.text_field :secretKey %>
  </div>

  <div class="field">
    <%= f.label :region %>
    <%= f.select :region, ['us-east-1', 'us-west-1','eu-west-1','eu-central-1','ap-southeast-1','ap-southeast-2','sa-east-1']  %>
  </div>

  <div class="field">
    <%= f.label :default %>
    <%= f.check_box :default %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
