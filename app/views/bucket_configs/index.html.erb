<p id="notice"><%= notice %></p>

<h1>Bucket Configs</h1>

<table>
  <thead>
    <tr>
      <th>Bucketname</th>
      <th>Servicename</th>
      <th>Region</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bucket_configs.each do |bucket_config| %>
      <tr>
        <td><%= bucket_config.bucketname %></td>
        <td><%= bucket_config.servicename %></td>
        <td><%= bucket_config.region %></td>
        <td><%= bucket_config.user %></td>
        <td><%= link_to 'Show', bucket_config %></td>
        <td><%= link_to 'Edit', edit_bucket_config_path(bucket_config) %></td>
        <td><%= link_to 'Destroy', bucket_config, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bucket Config', new_bucket_config_path %>
